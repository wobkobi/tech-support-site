    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "6.19.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.17",
    "to-valid-identifier": "^1.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
