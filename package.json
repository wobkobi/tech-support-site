    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^6.18.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.16",
    "to-valid-identifier": "^1.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
