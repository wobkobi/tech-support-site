{
  "name": "tech-support-site",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "postinstall": "prisma generate",
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "db:push": "prisma db push",
    "seed": "tsx prisma/seed.ts",
    "lint": "eslint \"{src,tests,scripts}/**/*.{js,ts,jsx,tsx}\"",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,css,md,json}\"",
    "build:icons": "tsx scripts/build-icons/index.ts",
    "build:poster": "npx tsx scripts/export-poster-screenshot.ts",
    "build:all": "npm run build:icons && npm run build",
    "test": "vitest"
  },
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "autoprefixer": "^10.4.24",
    "clsx": "^2.1.1",
    "googleapis": "^171.4.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "resend": "^6.9.2",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@next/bundle-analyzer": "^16.1.6",
    "@prisma/client": "6.19.2",
    "@tailwindcss/postcss": "^4.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.58.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "canvas": "^3.2.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^62.7.0",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "pdf-lib": "^1.17.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "6.19.2",
    "puppeteer": "^24.37.5",
    "qr-code-styling": "^1.9.2",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10.0.0"
  },
  "overrides": {
    "@typescript-eslint/typescript-estree": {
      "minimatch": "^10.2.1"
    },
    "glob": {
      "minimatch": "^10.2.1"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --max-warnings=0",
      "vitest related --run"
    ],
    "*.{css,html,json,md}": "prettier --write"
  }
}
